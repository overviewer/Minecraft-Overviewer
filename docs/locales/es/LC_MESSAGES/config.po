msgstr ""

#: ../../docs/config.rst:1163
msgid "Lighting"
msgstr ""

#: ../../docs/config.rst:1152
msgid "Applies lighting to each block."
msgstr ""

#: ../../docs/config.rst:1157
msgid "strength"
msgstr ""

#: ../../docs/config.rst:1157
msgid "how dark to make the shadows. from 0.0 to 1.0. Default: 1.0"
